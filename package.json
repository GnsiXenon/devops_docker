{
  "name": "geoguessr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "lint": "next lint"
  },
  "dependencies": {
    "next": "^14.2.24",
    "node-fetch": "^3.3.2",
    "react": "^18",
    "react-dom": "^18",
    "react-toastify": "^9.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/supertest": "^6.0.2",
    "autoprefixer": "^10.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.2.6",
    "typescript": "^5"
  },
  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "preset": "ts-jest",
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy",
      "\\.svg$": "<rootDir>/__mocks__/svgrMock.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom"
    ],
    "testMatch": [
      "**/?(*.)+(spec|test).[tj]s?(x)",
      "**/*.tsx"
    ]
  }
}